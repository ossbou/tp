pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                // Récupérer le code source depuis Git
                git https://github.com/ossbou/tp.git
            }
        }
        
        stage('Display Date') {
            steps {
                // Afficher la date système
                script {
                    def currentDate = new Date()
                    echo "Date système : ${currentDate}"
                }
            }
        }
        
        // Ajoutez d'autres étapes pour le build, les tests, le déploiement, etc.
    }
